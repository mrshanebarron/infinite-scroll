{
    "name": "mrshanebarron/infinite-scroll",
    "description": "Infinite scroll component for Laravel - supports Livewire and Vue",
    "type": "library",
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "MrShaneBarron\\InfiniteScroll\\": "src/"
        }
    },
    "extra": {
