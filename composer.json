{
    "name": "mrshanebarron/infinite-scroll",
    "description": "Infinite scroll component for Laravel",
    "keywords": ["laravel", "infinite", "scroll", "load", "livewire", "vue"],
    "license": "MIT",
    "authors": [{"name": "Shane Barron", "email": "mrshanebarron@gmail.com"}],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {"LaravelDesign\\infinite-scroll\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"LaravelDesign\\infinite-scroll\\Tests\\": "tests/"}
    },
    "extra": {
        "laravel": {
            "providers": ["LaravelDesign\\infinite-scroll\\infinite-scrollServiceProvider"]
        }
    },
    "config": {"sort-packages": true},
    "minimum-stability": "stable",
    "prefer-stable": true
}
